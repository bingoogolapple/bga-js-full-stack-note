{
  "name": "monorepo-npm-workspaces",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "workspaces": {
        "packages": [
          "packages/*"
        ]
      }
    },
    "node_modules/@monorepo-npm-workspaces/package-a": {
      "resolved": "packages/package-a",
      "link": true
    },
    "node_modules/@monorepo-npm-workspaces/package-b": {
      "resolved": "packages/package-b",
      "link": true
    },
    "packages/package-a": {
      "name": "@monorepo-npm-workspaces/package-a",
      "version": "1.0.0",
      "license": "MIT"
    },
    "packages/package-b": {
      "name": "@monorepo-npm-workspaces/package-b",
      "version": "1.0.0",
      "license": "MIT",
      "dependencies": {
        "@monorepo-npm-workspaces/package-a": "1.0.0"
      }
    }
  },
  "dependencies": {
    "@monorepo-npm-workspaces/package-a": {
      "version": "file:packages/package-a"
    },
    "@monorepo-npm-workspaces/package-b": {
      "version": "file:packages/package-b",
      "requires": {
        "@monorepo-npm-workspaces/package-a": "1.0.0"
      }
    }
  }
}
